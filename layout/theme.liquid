  {% if settings.animate_page_transitions %}
    <script type="text/javascript">window.setTimeout(function() { document.body.className += " loaded"; }, 25);</script>
  {% endif %}

  <a class="in-page-link visually-hidden skip-link" href="#MainContent">{{ 'general.accessibility.skip_to_content' | t }}</a>

  <div id="PageContainer" class="page-container">
    <div class="transition-body">

    {%- section 'header' -%}
    {% section 'newsletter-popup' %}

      <main class="main-content" id="MainContent">
        {{ content_for_layout }}
      </main>

      {%- section 'footer-promotions' -%}
      {%- section 'footer' -%}

    </div>
  </div>

  {%- liquid
    render 'video-modal'
    render 'photoswipe-template'
    render 'tool-tip'
  -%}
  
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.1/jquery.min.js"></script>
<script>
if($(window).width() >= 600){
    $(".FilterDrawer-normal").removeClass('drawer drawer--left');
}else{
    $('.filter-form .collapsible-trigger').prop('disabled', false);
    $(document).on('click', '.filter-form .collapsible-content__inner li', '.tag-list--active-tags li', function(){
        setTimeout(function(){
            $('.filter-form .collapsible-trigger').prop('disabled', false);
        }, 1000)
    })
    $(document).on('click', 'li.tag--remove a', function(){
        setTimeout(function(){
            $('.filter-form .collapsible-trigger').prop('disabled', false);
        }, 1000)
    })
}
</script>
